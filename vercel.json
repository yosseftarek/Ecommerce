{
    "version": 2,
    "builds": [
      {
        "src": "./api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "functions": {
      "api/*": {
        "maxDuration": 10,
        "memory": 1024,
        "maxRequestSize": "11mb"
      }
    }
  }
  